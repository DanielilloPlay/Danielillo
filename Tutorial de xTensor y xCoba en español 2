In[79]:= (*Las ecuaciones de Bianchi*)

cb = {l[a] cd[-a]@ps1 - m[a] cd[-a]@ps0, l[a] cd[-a]@ps2 - mb[a] cd[-a]@ps1, 
      l[a] cd[-a]@ps3 - mb[a] cd[-a]@ps2, l[a] cd[-a]@ps4 - mb[a] cd[-a]@ps3, 
      n[a] cd[-a]@ps0 - m[a] cd[-a]@ps1, n[a] cd[-a]@ps1 - m[a] cd[-a]@ps2, 
      n[a] cd[-a]@ps2 - m[a] cd[-a]@ps3, n[a] cd[-a]@ps3 - m[a] cd[-a]@ps4} // 
     ToBasisExpand // ContractBasis // Simplify;
ib = {-3 \[Kappa] ps2 + (2 \[Epsilon] + 4 \[Rho]) ps1 - (-p + 
        4 \[Alpha]) ps0, -2 \[Kappa] ps3 + 
     3 \[Rho] ps2 - (-2 p + 
        2 \[Alpha]) ps1 - \[Lambda] ps0, -\[Kappa] ps4 - (2 \[Epsilon] - 
        2 \[Rho]) ps3 + 3 p ps2 - 
     2 \[Lambda] ps1, -(4 \[Epsilon] - \[Rho]) ps4 + (4 p + 2 \[Alpha]) ps3 - 
     3 \[Lambda] ps2,
    (4 \[Gamma] - \[Mu]) ps0 - (4 \[Tau] + 2 \[Beta]) ps1 + 
     3 \[Sigma] ps2, \[Nu] ps0 + (2 \[Gamma] - 2 \[Mu]) ps1 - 3 \[Tau] ps2 + 
     2 \[Sigma] ps3, 
    2 \[Nu] ps1 - 3 \[Mu] ps2 + (2 \[Beta] - 2 \[Tau]) ps3 + \[Sigma] ps4, 
    3 \[Nu] ps2 - (2 \[Gamma] + 4 \[Mu]) ps3 + (-\[Tau] + 4 \[Beta]) ps4} // 
   Simplify;

In[81]:= (*Eliminos los 0*)

In[82]:= ib0 = Partition[DeleteCases[Flatten[ib], 0], 1];
cb0 = Partition[DeleteCases[Flatten[cb], 0], 1];

In[84]:= vectorAsignado7 = Table[cb0[[i]] -> ib0[[i]], {i, Length[cb0]}];

MatrixForm[vectorAsignado7]


