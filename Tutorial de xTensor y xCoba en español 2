In[26]:= weyl = Weyl[cd][{-a, -ch}, {-b, -ch}, {-c, -ch}, {-d, -ch}];

In[27]:= (*La base tetrada*)
l = CTensor[{Exp[Ar[r[]]]/Sqrt[2], Exp[Br[r[]]]/Sqrt[2], 0, 0}, {-ch}];
n = CTensor[{Exp[Ar[r[]]]/Sqrt[2], -Exp[Br[r[]]]/Sqrt[2], 0, 0}, {-ch}];
m = CTensor[{0, 0, r[]/Sqrt[2], I Sin[\[Theta][]] r[]/Sqrt[2]}, {-ch}];
mb = CTensor[{0, 0, r[]/Sqrt[2], -I Sin[\[Theta][]] r[]/Sqrt[2]}, {-ch}];
 (*Comprobamos que el escalar es de tipo tiempo*)

In[31]:= l[-a] n[a]

Out[31]= -1

In[32]:= mb[a] m[-a]

l[-a] m[a]


Out[32]= 1

Out[33]= 0

In[34]:= (*Definimos los coeficientes de Spin*)

\[Kappa] = cd[-a]@l[-b] m[b] l[a]; p = -cd[-a]@n[-b] mb[b] l[a];
\[Epsilon] = 1/2 (cd[-a]@l[-b] n[b] l[a] - cd[-a]@m[-b] mb[b] l[a]);
\[Rho] = cd[-a]@l[-b] m[b] mb[a];
\[Lambda] = -cd[-a]@n[-b] mb[b] mb[a];
\[Alpha] = 1/2 (cd[-a]@l[-b] n[b] mb[a] - cd[-a]@m[-b] mb[b] mb[a]);
\[Sigma] = cd[-a]@l[-b] m[b] m[a];
\[Mu] = -cd[-a]@n[-b] mb[b] m[a];
\[Beta] = 1/2 (cd[-a]@l[-b] n[b] m[a] - cd[-a]@m[-b] mb[b] m[a]);
\[Nu] = -cd[-a]@n[-b] mb[b] n[a];
\[Tau] = cd[-a]@l[-b] m[b] n[a];
\[Gamma] = 1/2 (cd[-a]@l[-b] n[b] n[a] - cd[-a]@m[-b] mb[b] n[a]);


In[45]:= esc = {p, \[Alpha], \[Beta], \[Gamma], \[Epsilon], \[Kappa], \[Lambda], \
\[Mu], \[Nu], \[Sigma], \[Tau], \[Rho]};
(*Paso 1:Crear el vector de letras *)
vector = Characters[
   "\[Pi]\[Alpha]\[Beta]\[Gamma]\[Epsilon]\[Kappa]\[Lambda]\[Mu]\[Nu]\[Sigma]\
\[Tau]\[Rho]"];  (*Vector de letras*)
(*Paso 3:Combinar las letras con sus valores en pares*)
vectorAsignado = Table[vector[[i]] -> esc[[i]], {i, Length[vector]}];
(*Paso 4:Mostrar el resultado en forma de matriz*)
MatrixForm[vectorAsignado]


