In[54]:= (*Obtenemos las derivadas de \[CapitalDelta]*)
\[CapitalDelta] = Map[n[a] cd[-a]@# &, esc]  // ToBasisExpand // ContractBasis;
vector2 = Table[Symbol["\[CapitalDelta]" <> ToString[i]], {i, vector}];
vectorAsignado2 = 
  Table[vector2[[i]] -> \[CapitalDelta][[i]], {i, Length[vector2]}];
(*Paso 4:Mostrar el resultado en forma de matriz*)
MatrixForm[vectorAsignado2]

