In[66]:= (*Componentes del tensor de Weyl*)
ps0 = -Weyl[cd][-a, -b, -c, -d] l[a] m[b] l[c] m[d];
ps1 = -Weyl[cd][-a, -b, -c, -d] l[a] n[b] l[c] m[d];
ps2 = -1/2 Weyl[cd][-a, -b, -c, -d] (l[a] n[b] l[c] n[d] - 
     l[a] n[b] m[c] mb[d]);
ps3 = Weyl[cd][-a, -b, -c, -d] l[a] n[b] n[c] mb[d];
ps4 = -Weyl[cd][-a, -b, -c, -d] n[a] mb[b] n[c] mb[d];


In[71]:= (*Como estamos en vac√≠o, todas las componentes del Ricci y el escalar de \
curvatura ser√°n 0*)

In[72]:= (*Los conmutadores van en orden como va en el trabajo del TFG*)

In[73]:= comm = -{DD[[12]] - \[Stigma][[6]], DD[[10]] - \[Delta][[6]], 
     DD[[11]] - \[CapitalDelta][[6]], DD[[2]] - \[Stigma][[5]], 
     DD[[3]] - \[Delta][[5]], DD[[4]] - \[CapitalDelta][[5]], 
     DD[[7]] - \[Stigma][[1]], DD[[8]] - \[Delta][[1]], 
     DD[[9]] - \[CapitalDelta][[1]], \[CapitalDelta][[7]] - \[Stigma][[
       9]], \[Delta][[12]] - \[Stigma][[10]], \[Delta][[2]] - \[Stigma][[
       3]], \[Delta][[7]] - \[Stigma][[8]], \[Delta][[9]] - \[CapitalDelta][[
       8]], \[Delta][[4]] - \[CapitalDelta][[3]], \[Delta][[
       11]] - \[CapitalDelta][[10]], \[CapitalDelta][[12]] - \[Stigma][[
       11]], \[CapitalDelta][[2]] - \[Stigma][[4]]} // Simplify;

comm1 = {\[Rho]^2 + \[Sigma] Dagger[\[Sigma]] + (\[Epsilon] + 
        Dagger[\[Epsilon]]) \[Rho] - 
     Dagger[\[Kappa]] \[Tau] - \[Kappa] (3 \[Alpha] - Dagger[\[Beta]] - 
        p), (\[Rho] + Dagger[\[Rho]]) \[Sigma] (3 \[Epsilon] - 
        Dagger[\[Epsilon]]) \[Sigma] - (\[Tau] - Dagger[p] + 
        Dagger[\[Alpha]] + 3 \[Beta]) \[Kappa] + 
     ps0, (\[Tau] + Dagger[p]) \[Rho] + (Dagger[\[Tau]] + 
        p) \[Sigma] + (\[Epsilon] - Dagger[\[Epsilon]]) \[Tau] - (3 \[Gamma] +
         Dagger[\[Gamma]]) \[Kappa] + 
     ps1, (\[Rho] - 2 \[Epsilon] + 
        Dagger[\[Epsilon]]) \[Alpha] + \[Beta] Dagger[\[Sigma]] - 
     Dagger[\[Beta]] \[Epsilon] - \[Kappa] \[Lambda] - 
     Dagger[\[Kappa]] \[Gamma] + (\[Epsilon] + \[Rho]) p, (\[Alpha] + 
        p) \[Sigma] + (Dagger[\[Rho]] - 
        Dagger[\[Epsilon]]) \[Beta] - (\[Mu] + \[Gamma]) \[Kappa] - (Dagger[\
\[Alpha]] - Dagger[p]) \[Epsilon] + 
     ps1, (\[Tau] + Dagger[p]) \[Alpha] + (Dagger[\[Tau]] + 
        p) \[Beta] - (\[Epsilon] + Dagger[\[Epsilon]]) \[Gamma] - (\[Gamma] + 
        Dagger[\[Gamma]]) \[Epsilon] + \[Tau] p - \[Nu] \[Kappa] + 
     ps2, (\[Rho] \[Lambda] + Dagger[\[Sigma]] \[Mu]) + 
     p^2 + (\[Alpha] - 
        Dagger[\[Beta]]) p - \[Nu] Dagger[\[Kappa]] - (3 \[Epsilon] - 
        Dagger[\[Epsilon]]) \[Lambda], (Dagger[\[Rho]] \[Mu] + \[Sigma] \
\[Lambda]) + p Dagger[p] - (\[Epsilon] + Dagger[\[Epsilon]]) \[Mu] - 
     p (Dagger[\[Alpha]] - \[Beta]) - \[Nu] \[Kappa] + 
     ps2, (p + 
        Dagger[\[Tau]]) \[Mu] + (Dagger[p] + \[Tau]) \[Lambda] + (\[Gamma] - 
        Dagger[\[Gamma]]) p - (3 \[Epsilon] + Dagger[\[Epsilon]]) \[Nu] + 
     ps3, -(\[Mu] + Dagger[\[Mu]]) \[Lambda] - (3 \[Gamma] - 
        Dagger[\[Gamma]]) \[Lambda] + (3 \[Alpha] + Dagger[\[Beta]] + p - 
        Dagger[\[Tau]]) \[Nu] - 
     ps4, \[Rho] (Dagger[\[Alpha]] + \[Beta]) - \[Sigma] (3 \[Alpha] - 
        Dagger[\[Beta]]) + (\[Rho] - Dagger[\[Rho]]) \[Tau] + (\[Mu] - 
        Dagger[\[Mu]]) \[Kappa] - ps1, (\[Mu] \[Rho] - \[Lambda] \[Sigma]) + 
     Dagger[\[Alpha]] \[Alpha] + Dagger[\[Beta]] \[Beta] - 
     2 \[Alpha] \[Beta] + \[Gamma] (\[Rho] - 
        Dagger[\[Rho]]) + \[Epsilon] (\[Mu] - Dagger[\[Mu]]) - 
     ps2, (\[Rho] - Dagger[\[Rho]]) \[Nu] + (\[Mu] - 
        Dagger[\[Mu]]) p + \[Mu] (\[Alpha] + 
        Dagger[\[Beta]]) + \[Lambda] (Dagger[\[Alpha]] - 3 \[Beta]) - 
     ps3, \[Mu]^2 + 
     Dagger[\[Lambda]] \[Lambda] + \[Mu] (Dagger[\[Gamma]] + \[Gamma]) - 
     Dagger[\[Nu]] p + (\[Tau] - 3 \[Beta] - 
        Dagger[\[Alpha]]) \[Nu], \[Gamma] (\[Tau] - 
        Dagger[\[Alpha]] - \[Beta]) + \[Mu] \[Tau] - \[Sigma] \[Nu] - 
     Dagger[\[Nu]] \[Epsilon] - \[Beta] (\[Gamma] - 
        Dagger[\[Gamma]] - \[Mu]) + \[Alpha]  Dagger[\[Lambda]], \[Mu] \
\[Sigma] + 
     Dagger[\[Lambda]] \[Rho] + \[Tau] (\[Tau] + \[Beta] - 
        Dagger[\[Alpha]]) - \[Sigma] (3 \[Gamma] - Dagger[\[Gamma]]) - 
