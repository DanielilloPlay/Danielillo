In[87]:= comm0[[1]] == comm10[[1]] // Simplify

Out[87]= {(E^(-2 Br[
r[]]) (-cte E^(2 (Ar[
r[]] + Br[
r[]])) r[] + Derivative[1][Ar][
r[]] + Derivative[1][Br][
r[]]))/(2 r[])} == {0}

In[88]:= -((cte E^(-2 Ar[
r[]]) r[] - E^(-2 Br[
r[]]) Derivative[1][Ar][
r[]] - E^(-2 Br[
r[]]) Derivative[1][Br][
r[]])/(2 r[])) == 0 // Simplify

Out[88]= cte E^(2 Br[
r[]]) r[] == E^(2 Ar[
r[]]) (Derivative[1][Ar][
r[]] + Derivative[1][Br][
r[]])

In[89]:= 

In[90]:= 

In[91]:= (E^(-2 Br[
r[]]) (-1 + cte r[]^2 + r[] Derivative[1][Ar][
r[]]))/(2 r[]^2) == (E^(-2 Br[
r[]]) + cte E^(-2 Ar[
r[]]) r[]^2 - E^(-2 Br[
r[]]) r[] Derivative[1][Ar][
r[]])/(2 r[]^2) // Simplify

Out[91]= E^(2 Ar[
r[]]) (-2 + cte r[]^2 + 2 r[] Derivative[1][Ar][
r[]]) == cte E^(2 Br[
r[]]) r[]^2

In[92]:= 

In[93]:= -((E^(-2 Br[
r[]]) (1 + r[] Derivative[1][Br][
r[]]))/(2 r[]^2)) == -(cte/(2 r[]^2)) // Simplify

Out[93]= cte E^(2 Br[
r[]]) == 1 + r[] Derivative[1][Br][
r[]]

In[94]:= (cte - E^(-2 Br[r[]]) (1 + r[] Derivative[1][Br][r[]]))/(
  2 r[]^2) == -((cte E^(-2 Ar[
r[]]) r[] - E^(-2 Br[
r[]]) Derivative[1][Ar][
r[]] - E^(-2 Br[
r[]]) Derivative[1][Br][
r[]])/(2 r[])) // Simplify

Out[94]= cte E^(2 Br[
r[]]) (E^(2 Ar[
r[]]) + r[]^2) == E^(2 Ar[
r[]]) (1 + r[] Derivative[1][Ar][
r[]] + 2 r[] Derivative[1][Br][
r[]])

In[95]:=  

