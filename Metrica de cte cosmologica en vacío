In[3]:= DefManifold[M, 4, IndexRange[a, q]];
DefChart[ch, M, {0, 1, 2, 3}, {t[], r[], \[Theta][], \[Phi][]}, 
  ChartColor -> Blue];
DefConstantSymbol[mass, PrintAs -> "M"];
DefConstantSymbol[cte, PrintAs -> "\[CapitalLambda]"];
(*Las funciones que hay que hallar*)
DefScalarFunction[Ar];
DefScalarFunction[Br];
$Assumptions = 
  mass > 0 && r[] > 2 mass && 0 < \[Theta][] < \[Pi] && 
   0 < \[Phi][] < 2 \[Pi] && t[] \[Element] Reals; 
g = CTensor[{{-Exp[2 Ar[r[]]], 0, 0, 0}, {0, Exp[2 Br[r[]]], 0, 
     0}, {0, 0, r[]^2, 0}, {0, 0, 0, 
     r[]^2 Sin[\[Theta][]]^2}}, {-ch, -ch}];
SetCMetric[g, ch, SignatureOfMetric -> {1, 3, 0}];

