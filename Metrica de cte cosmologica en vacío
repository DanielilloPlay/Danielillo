In[80]:= (*Las ecuaciones de Bianchi*)

cb = {l[a] cd[-a]@\[Psi]1 - m[a] cd[-a]@\[Psi]0, 
      l[a] cd[-a]@\[Psi]2 - mb[a] cd[-a]@\[Psi]1, 
      l[a] cd[-a]@\[Psi]3 - mb[a] cd[-a]@\[Psi]2, 
      l[a] cd[-a]@\[Psi]4 - mb[a] cd[-a]@\[Psi]3, 
      n[a] cd[-a]@\[Psi]0 - m[a] cd[-a]@\[Psi]1, 
      n[a] cd[-a]@\[Psi]1 - m[a] cd[-a]@\[Psi]2, 
      n[a] cd[-a]@\[Psi]2 - m[a] cd[-a]@\[Psi]3, 
      n[a] cd[-a]@\[Psi]3 - m[a] cd[-a]@\[Psi]4} // ToBasisExpand // 
    ContractBasis // Simplify;
ib = {-3 \[Kappa] \[Psi]2 + (2 \[Epsilon] + 4 \[Rho]) \[Psi]1 - (-p + 
        4 \[Alpha]) \[Psi]0, -2 \[Kappa] \[Psi]3 + 
     3 \[Rho] \[Psi]2 - (-2 p + 
        2 \[Alpha]) \[Psi]1 - \[Lambda] \[Psi]0, -\[Kappa] \[Psi]4 - \
(2 \[Epsilon] - 2 \[Rho]) \[Psi]3 + 3 p \[Psi]2 - 
     2 \[Lambda] \[Psi]1, -(4 \[Epsilon] - \[Rho]) \[Psi]4 + (4 p + 
        2 \[Alpha]) \[Psi]3 - 3 \[Lambda] \[Psi]2,
    (4 \[Gamma] - \[Mu]) \[Psi]0 - (4 \[Tau] + 2 \[Beta]) \[Psi]1 + 
     3 \[Sigma] \[Psi]2, \[Nu] \[Psi]0 + (2 \[Gamma] - 
        2 \[Mu]) \[Psi]1 - 3 \[Tau] \[Psi]2 + 2 \[Sigma] \[Psi]3, 
    2 \[Nu] \[Psi]1 - 
     3 \[Mu] \[Psi]2 + (2 \[Beta] - 
        2 \[Tau]) \[Psi]3 + \[Sigma] \[Psi]4, 
    3 \[Nu] \[Psi]2 - (2 \[Gamma] + 4 \[Mu]) \[Psi]3 + (-\[Tau] + 
        4 \[Beta]) \[Psi]4} // Simplify;

In[82]:= (*Eliminos los 0*)

In[83]:= ib0 = Partition[DeleteCases[Flatten[ib], 0], 1];
cb0 = Partition[DeleteCases[Flatten[cb], 0], 1];

In[85]:= 
vectorAsignado7 = Table[cb0[[i]] -> ib0[[i]], {i, Length[cb0]}];
MatrixForm[vectorAsignado7]


