In[25]:= (*El tensor de Weyl*)
list5 = ComponentArray[
   Weyl[cd][{-a, -ch}, {-b, -ch}, {-c, -ch}, {-d, -ch}]];
list6 = Table[
   If[
    UnsameQ[list5[[i, j, k, l]], 
     0], {Subscript["C", i - 1, j - 1, k - 1, l - 1]
     , list5[[i, j, k, l]]}
    ],
   {i, 1, Length[list5]}, {j, 1, i - 1}, {k, 1, Length[list5]}, {l, 1,
     k - 1}
   ];
Partition[DeleteCases[Flatten[list6], Null], 2] // TableForm

