In[28]:= \[Psi]0 = -Weyl[cd][-a, -b, -c, -d] l[a] m[b] l[c] m[d];
\[Psi]1 = -Weyl[cd][-a, -b, -c, -d] l[a] n[b] l[c] m[d];
\[Psi]2 = 
  1/2 Weyl[cd][-a, -b, -c, -d] (l[a] n[b] l[c] n[d] + 
     l[a] n[b] m[c] mb[d]);
\[Psi]3 = Weyl[cd][-a, -b, -c, -d] l[a] n[b] n[c] mb[d];
\[Psi]4 = -Weyl[cd][-a, -b, -c, -d] n[a] mb[b] n[c] mb[d];

In[117]:= 
alp = (-\[Epsilon] HM[t[], 
    z[]])/( \[Epsilon] Hm[t[], z[]] - \[Epsilon] Sqrt[
     Hm[t[], z[]]^2 +  HM[t[], z[]]^2]);
NN = 1/Sqrt[1 + alp^2];

mx = 1/Sqrt[1 - \[Epsilon] Sqrt[Hm[t[], z[]]^2 +  HM[t[], z[]]^2]];
my =  1/Sqrt[1 + \[Epsilon] Sqrt[Hm[t[], z[]]^2 +  HM[t[], z[]]^2]];

MX = NN (alp mx + I my);
MY = NN (mx - I alp my);


In[123]:= (*La base tetrada*)
l = CTensor[{1/Sqrt[2], 0, 0, 1/Sqrt[2]}, {-ch}];
n = CTensor[{
    1/Sqrt[2], 0, 0, -1/Sqrt[2]}, {-ch}];
m = CTensor[{0, MX/Sqrt[2], MY/Sqrt[2], 0}, {-ch}];
mb = CTensor[{0, Dagger[MX]/Sqrt[2], Dagger[MY]/Sqrt[2] , 0}, {-ch}];

