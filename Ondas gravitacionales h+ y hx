In[21]:= list3 = ComponentArray[Ricci[cd][{-a, -ch}, {-b, -ch}]];
list4 = Table[
   If[
    UnsameQ[list3[[i, j]], 0], {Subscript["R", i - 1, j - 1]
     , list3[[i, j]]}
    ],
   {i, 1, Length[list3]}, {j, 1, i}
   ];
Partition[DeleteCases[Flatten[list4], Null], 2] // TableForm

