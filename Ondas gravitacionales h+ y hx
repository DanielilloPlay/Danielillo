In[18]:= 
list1 = ComponentArray[
   Riemann[cd][{a, ch}, {-b, -ch}, {-c, -ch}, {-d, -ch}]];
list2 = Table[
   If[
    UnsameQ[list1[[i, j, k, l]], 
     0], {Subscript[Superscript["R", i - 1], j - 1, k - 1, l - 1]
     , list1[[i, j, k, l]]}
    ],
   {i, 1, Length[list1]}, {j, 1, Length[list1]}, {k, 1, 
    Length[list1]}, {l, 1, k - 1}
   ];
Partition[DeleteCases[Flatten[list2], Null], 2] // TableForm


