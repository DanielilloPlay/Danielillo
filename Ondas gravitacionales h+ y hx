In[83]:= (*Definimos los coeficientes de Spin y linealizamos*)

In[84]:= \[Kappa] = 
  Normal[Series[cd[-a]@l[-b] m[b] l[a], {\[Epsilon], 0, 1}]] // 
   Simplify;
p = Normal[Series[-cd[-a]@n[-b] mb[b] l[a], {\[Epsilon], 0, 1}]] // 
   Simplify;
ep = Normal[
    Series[1/
      2 (cd[-a]@l[-b] n[b] l[a] - 
        cd[-a]@m[-b] mb[b] l[a]), {\[Epsilon], 0, 1}]] // Simplify;
\[Rho] = 
  Normal[Series[cd[-a]@l[-b] m[b] mb[a], {\[Epsilon], 0, 1}]] // 
   Simplify;
\[Lambda] = 
  Normal[Series[-cd[-a]@n[-b] mb[b] mb[a], {\[Epsilon], 0, 1}]] // 
   Simplify;
\[Alpha] = 
  Normal[Series[
     1/2 (cd[-a]@l[-b] n[b] mb[a] - 
        cd[-a]@m[-b] mb[b] mb[a]), {\[Epsilon], 0, 1}]] // Simplify;
\[Sigma] = 
  Normal[Series[cd[-a]@l[-b] m[b] m[a], {\[Epsilon], 0, 1}]] // 
   Simplify;
\[Mu] = Normal[Series[-cd[-a]@n[-b] mb[b] m[a], {\[Epsilon], 0, 1}]] // 
   Simplify;
\[Beta] = 
  Normal[Series[
     1/2 (cd[-a]@l[-b] n[b] m[a] - 
        cd[-a]@m[-b] mb[b] m[a]), {\[Epsilon], 0, 1}]] // Simplify;
\[Nu] = Normal[Series[-cd[-a]@n[-b] mb[b] n[a], {\[Epsilon], 0, 1}]] // 
   Simplify;
\[Tau] = 
  Normal[Series[cd[-a]@l[-b] m[b] n[a], {\[Epsilon], 0, 1}]] // 
   Simplify;
\[Gamma] = 
  Normal[Series[
     1/2 (cd[-a]@l[-b] n[b] n[a] - 
        cd[-a]@m[-b] mb[b] n[a]), {\[Epsilon], 0, 1}]] // Simplify;
esc = {p, \[Alpha], \[Beta], \[Gamma], 
   ep, \[Kappa], \[Lambda], \[Mu], \[Nu], \[Sigma], \[Tau], \[Rho]};
(*Paso 1:Crear el vector de letras *)
vector = 
  Characters[
   "\[Pi]\[Alpha]\[Beta]\[Gamma]\[Epsilon]\[Kappa]\[Lambda]\[Mu]\[Nu]\
\[Sigma]\[Tau]\[Rho]"];  (*Vector de letras*)
(*Paso 3:Combinar las letras con sus valores en pares*)
vectorAsignado = Table[vector[[i]] -> esc[[i]], {i, Length[vector]}];
(*Paso 4:Mostrar el resultado en forma de matriz*)
MatrixForm[vectorAsignado]

